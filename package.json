{
  "name": "webgl-sprites",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "concurrently 'npm:dev:js' 'npm:dev:html'",
    "dev:js": "node dev",
    "dev:html": "eleventy --serve",
    "lint": "npm run lint:js -s",
    "lint:js": "eslint src",
    "lint:fix": "eslint --fix src",
    "test": "jest",
    "build:types": "tsc -d --emitDeclarationOnly",
    "build:js": "node build",
    "build:html": "eleventy",
    "build:docs": "typedoc src/index.ts && touch docs/.nojekyll",
    "build": "rm -rf dist && npm run build:html -s && npm run build:types -s && npm run build:js -s",
    "prepublishOnly": "npm run lint -s && npm test -s && npm run build",
    "semantic-release": "semantic-release"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "browser-sync": "^2.29.1",
    "concurrently": "^8.0.1",
    "esbuild": "^0.17.18",
    "esbuild-plugin-glsl": "^1.2.1",
    "eslint": "^8.39.0",
    "jest": "^29.5.0",
    "jest-webgl-canvas-mock": "^0.2.3",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.24.6",
    "typescript": "^5.0.4"
  }
}
